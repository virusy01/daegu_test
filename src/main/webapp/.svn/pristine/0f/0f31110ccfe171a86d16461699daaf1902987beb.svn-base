package com.sck.model;

import java.util.Date;

public class CorpHelp
{
	private Integer corpSeq;
	private Integer helpSeq;
	private String helper;
	private String prjNm;
	private Date helpStDt;
	private Date helpEndDt;
	private Integer helpAmt;
	private Date creDt;
	private Date modDt;
	public Integer getCorpSeq()
	{
		return corpSeq;
	}
	public void setCorpSeq(Integer corpSeq)
	{
		this.corpSeq = corpSeq;
	}
	public Integer getHelpSeq()
	{
		return helpSeq;
	}
	public void setHelpSeq(Integer helpSeq)
	{
		this.helpSeq = helpSeq;
	}
	public String getHelper()
	{
		return helper;
	}
	public void setHelper(String helper)
	{
		this.helper = helper;
	}
	public String getPrjNm()
	{
		return prjNm;
	}
	public void setPrjNm(String prjNm)
	{
		this.prjNm = prjNm;
	}
	public Date getHelpStDt()
	{
		return helpStDt;
	}
	public void setHelpStDt(Date helpStDt)
	{
		this.helpStDt = helpStDt;
	}
	public Date getHelpEndDt()
	{
		return helpEndDt;
	}
	public void setHelpEndDt(Date helpEndDt)
	{
		this.helpEndDt = helpEndDt;
	}
	public Integer getHelpAmt()
	{
		return helpAmt;
	}
	public void setHelpAmt(Integer helpAmt)
	{
		this.helpAmt = helpAmt;
	}
	public Date getCreDt()
	{
		return creDt;
	}
	public void setCreDt(Date creDt)
	{
		this.creDt = creDt;
	}
	public Date getModDt()
	{
		return modDt;
	}
	public void setModDt(Date modDt)
	{
		this.modDt = modDt;
	}
	@Override
	public String toString()
	{
		return "CorpHelp [corpSeq=" + corpSeq + ", helpSeq=" + helpSeq
				+ ", helper=" + helper + ", prjNm=" + prjNm + ", helpStDt="
				+ helpStDt + ", helpEndDt=" + helpEndDt + ", helpAmt=" + helpAmt
				+ ", creDt=" + creDt + ", modDt=" + modDt + "]";
	}
	
}
