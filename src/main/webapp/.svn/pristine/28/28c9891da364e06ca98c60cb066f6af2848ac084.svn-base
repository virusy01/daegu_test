package com.sck.model;

import java.util.List;

public class MenuHierarchy extends Menu
{
	public MenuHierarchy(){super();}
	public MenuHierarchy(String menuCd)
	{
		super();
		setMenuCd(menuCd);
	}
	
	private List<MenuHierarchy> children;

	public List<MenuHierarchy> getChildren()
	{
		return children;
	}
	public void setChildren(List<MenuHierarchy> children)
	{
		this.children = children;
	}
	@Override
	public String toString()
	{
		return "MenuHierarchy [children=" + children + "]";
	}
	
	
}
