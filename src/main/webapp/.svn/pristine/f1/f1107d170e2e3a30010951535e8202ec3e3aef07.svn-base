<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Web Component Test : Fixture for px-dropdown</title>

    <!-- Requires Webcomponents.js polyfill is provided by the page for browsers that don't support html imports -->
    <script src="../bower_components/webcomponentsjs/webcomponents-lite.min.js"></script>
    <!-- web-component-tester automatically provides local copy of brower.js below
         for any URL ending with "/web-component-tester/browser.js"
         when wct is run from the command line -->
    <script src="../web-component-tester/browser.js"></script>
    <link rel="import" href="../px-dropdown.html" />
    <!-- Retain this link for px-theme.html -->
    <!-- For some reason px-theme is needed for WCT to run without some errors -->
    <link rel="import" href="../../px-theme/px-theme.html"/>
    <!-- This script link will contain the generated script for basic wct test cases -->
    <script src="px-dropdown-base-tests.js"></script>
    <!-- This script link will contain the generated script stub for custom, developer-added test cases -->
    <script src="px-dropdown-custom-tests.js"></script>
  </head>

  <body>
    <h3 id="title">Web Component Test : Fixture for px-dropdown</h3>
    <px-dropdown id="px_dropdown_1">
        <px-dropdown-content class="px-dropdown-content" extend-dropdown="true" extend-dropdown-by="20" max-cont-character-width="10" items='[{"key":"one", "val": "One"}, {"key":"two", "val": "Two"}, {"key":"three", "val": "Three"}, {"key":"four", "val": "How now brown cow"}]'>
      </px-dropdown-content>
    </px-dropdown>
    <px-dropdown id="px_dropdown_2" hide-chevron="true">
        <px-dropdown-content class="px-dropdown-content" extend-dropdown="true" extend-dropdown-by="20" max-cont-character-width="10" items='[{"key":"one", "val": "One"}, {"key":"two", "val": "Two"}, {"key":"three", "val": "Three"}, {"key":"four", "val": "How now brown cow"}]'>
      </px-dropdown-content>
    </px-dropdown>
    <px-dropdown id="px_dropdown_3" hide-chevron="true">
        <px-dropdown-content class="px-dropdown-content" extend-dropdown="true" extend-dropdown-by="20" max-cont-character-width="10" items='[{"key":"one", "val": "One"}, {"key":"two", "val": "Two"}, {"key":"three", "val": "Three"}, {"key":"four", "val": "How now brown cow"}]'>
      </px-dropdown-content>
    </px-dropdown>
    <px-dropdown id="px_dropdown_check" display-value="Text">
      <px-dropdown-content id="px_dropdown_content_check" class="px-dropdown-content" checkbox-mode="true" max-cont-character-width="10" items='[{"val": "One", "checked": true}, {"val": "Two", "checked": false}, {"val": "Three", "checked": "true"}, {"val": "four"}, {"val": "five"}, {"val": "six"}, {"val": "seven"}, {"val": "eight"}, {"val": "nine"}, {"val": "ten"}]'>
      </px-dropdown-content>
    </px-dropdown>
  </body>
</html>
