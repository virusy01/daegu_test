package com.sck.model;

import java.util.Date;

import com.fasterxml.jackson.annotation.JsonIgnore;

public class Menu
{
	private String menuCd;
	private String menuNm;
	private String parentMenuCd;
	private String menuIco;
	private String url;
	private String urlParam;
	private Integer sort;
	private boolean use;
	private boolean created;
	@JsonIgnore
	private Date creDt;
	public String getMenuCd()
	{
		return menuCd;
	}
	public void setMenuCd(String menuCd)
	{
		this.menuCd = menuCd;
	}
	public String getMenuNm()
	{
		return menuNm;
	}
	public void setMenuNm(String menuNm)
	{
		this.menuNm = menuNm;
	}
	public String getParentMenuCd()
	{
		return parentMenuCd;
	}
	public void setParentMenuCd(String parentMenuCd)
	{
		this.parentMenuCd = parentMenuCd;
	}
	public String getMenuIco()
	{
		return menuIco;
	}
	public void setMenuIco(String menuIco)
	{
		this.menuIco = menuIco;
	}
	public String getUrl()
	{
		return url;
	}
	public void setUrl(String url)
	{
		this.url = url;
	}
	public String getUrlParam()
	{
		return urlParam;
	}
	public void setUrlParam(String urlParam)
	{
		this.urlParam = urlParam;
	}
	public Integer getSort()
	{
		return sort;
	}
	public void setSort(Integer sort)
	{
		this.sort = sort;
	}
	public boolean isUse()
	{
		return use;
	}
	public void setUse(boolean use)
	{
		this.use = use;
	}
	public Date getCreDt()
	{
		return creDt;
	}
	public void setCreDt(Date creDt)
	{
		this.creDt = creDt;
	}
	public boolean isNew() {
		return created;
	}
	public void setNew(boolean created) {
		this.created = created;
	}
	@Override
	public String toString()
	{
		return "Menu [menuCd=" + menuCd + ", menuNm=" + menuNm
				+ ", parentMenuCd=" + parentMenuCd + ", menuIco=" + menuIco
				+ ", url=" + url + ", urlParam=" + urlParam + ", sort=" + sort
				+ ", use=" + use + ", creDt=" + creDt + "]";
	}
	
}
