            $("#corpDetail").css("display", "");
            $("#corpDetail2").css("display", "none");
            $("#selCorpType2").change(showExtraInput);
            $("#selBizSect2").change(showExtraInput);
        } else if (pageNo == 3) {
            $("#corpList").css("display", "none");
            $("#corpDetail").css("display", "none");
            $("#corpDetail2").css("display", "");
        } else {
            //nothing
        }
    }

